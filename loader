
local keyURL = "https://raw.githubusercontent.com/frizkemono/forvelt/refs/heads/main/key"
local player = game:GetService("Players").LocalPlayer
local gui = Instance.new("ScreenGui")
gui.Name = "KeyUI"
gui.ResetOnSpawn = false
gui.Parent = player:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 400, 0, 220)
frame.Position = UDim2.new(0.5, -200, 0.5, -110)
frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frame.Active = true
frame.Draggable = true
frame.Parent = gui
frame.ZIndex = 99

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
title.Text = "Enter Key"
title.TextColor3 = Color3.new(1, 1, 1)
title.Font = Enum.Font.SourceSansBold
title.TextSize = 22
title.ZIndex = 100
title.Parent = frame

local textBox = Instance.new("TextBox")
textBox.PlaceholderText = "Enter your key..."
textBox.Size = UDim2.new(0.8, 0, 0, 30)
textBox.Position = UDim2.new(0.1, 0, 0.3, 0)
textBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
textBox.TextColor3 = Color3.new(0, 0, 0)
textBox.Font = Enum.Font.SourceSans
textBox.TextSize = 18
textBox.Parent = frame
textBox.ZIndex = 100

local subText = Instance.new("TextLabel")
subText.Size = UDim2.new(0.8, 0, 0, 20)
subText.Position = UDim2.new(0.1, 0, 0.45, 0)
subText.BackgroundTransparency = 1
subText.Text = "new update: added shit ui ðŸ¤­ðŸ¤­"
subText.TextColor3 = Color3.new(1, 1, 1)
subText.Font = Enum.Font.SourceSans
subText.TextSize = 14
subText.TextScaled = true
subText.Parent = frame
subText.ZIndex = 100

local button = Instance.new("TextButton")
button.Size = UDim2.new(0.5, 0, 0, 30)
button.Position = UDim2.new(0.25, 0, 0.65, 0)
button.BackgroundColor3 = Color3.fromRGB(70, 130, 180)
button.Text = "Validate"
button.TextColor3 = Color3.new(1, 1, 1)
button.Font = Enum.Font.SourceSansBold
button.TextSize = 18
button.TextScaled = true
button.Parent = frame
button.ZIndex = 100

local message = Instance.new("TextLabel")
message.Size = UDim2.new(1, 0, 0, 20)
message.Position = UDim2.new(0, 0, 0.85, 0)
message.BackgroundTransparency = 1
message.Text = ""
message.TextColor3 = Color3.fromRGB(255, 50, 50)
message.Font = Enum.Font.SourceSans
message.TextSize = 16
message.TextScaled = true
message.Parent = frame
message.ZIndex = 100

local function getRemoteKey()
	local success, result = pcall(function()
		return game:HttpGet(keyURL)
	end)
	if success then
		return result:match("^%s*(.-)%s*$")
	else
		warn("Failed to fetch key: " .. tostring(result))
		return nil
	end
end

button.MouseButton1Click:Connect(function()
	local inputKey = textBox.Text
	local remoteKey = getRemoteKey()

	if not remoteKey then
		message.Text = "Failed to fetch key."
		message.TextColor3 = Color3.fromRGB(255, 100, 0)
		return
	end

	if inputKey == remoteKey then
		message.Text = "Key Accepted!"
		message.TextColor3 = Color3.fromRGB(0, 255, 0)
		wait(1)
		gui:Destroy()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/frizkemono/forvelt/refs/heads/main/kemono%20%3A3cc"))()
	else
		message.Text = "Invalid Key."
		message.TextColor3 = Color3.fromRGB(255, 50, 50)
		wait(2)
		player:Kick("Invalid key.")
	end
end)
